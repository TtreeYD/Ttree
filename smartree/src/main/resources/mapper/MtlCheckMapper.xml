<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.yedam.smartree.smartree.check.mapper.MtlCheckMapper">
  	<select id="Allselect" resultType="MtlCheckVO">
  		select c.mtl_date, n.bp_code, n.mtl_name
		from mtl_chk c join mtl_need n
		on c.mtl_code = n.mtl_code
  	</select>
  
  </mapper>